# Algorithm-Study
import java.util.*;
public class Main {

	public static void main(String[] args) {
		int h;		
		int m;	
		Scanner scan = new Scanner(System.in);
		h = scan.nextInt();
		m = scan.nextInt();
		
		int H = h;
		int M;
		if((m-45) < 0) {
			--H;
			M = (m + 60) - 45;
		} else {
			M = m - 45;
		}
		
		System.out.println(H + " " + M);
		

	}

}
